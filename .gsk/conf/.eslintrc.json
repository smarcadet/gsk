{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "modules" : true
    },
    "sourceType" : "module"
  },
  "globals": {
  },
  "plugins": [
  ],
  "rules": {
    "comma-dangle": ["error", "always"],
    "indent": ["error", 2]
  }
}
