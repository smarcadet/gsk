{
  "env": {
    "node": true,
    "browser": false,
    "es6": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "modules" : true
    },
    "sourceType" : "module"
  },
  "globals": {
    "console": true
  },
  "plugins": [
  ],
  "rules": {
    "comma-dangle": ["error", "ignore"],
    "indent": ["error", 2],
    "no-console": "off"
  }
}
